[alias]
	lg = log -p
	lgp = log --graph -p
	addall = add -A
	st = status
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	last = log -1 HEAD
	fe = fetch
	co = checkout
	subst = submodule status --recursive
	subup = submodule update --recursive
	subsync = submodule sync
	br = branch
	re = rebase
[clean]
	requiredForce = true
